<Header>
  <img
    class="header__logo"
    src="assets/images/logo.png"
    alt="Tomstagram logo"
  />
</Header>

<main class="aaa">
  {{#each model as |post|}}
    <Post @post={{post}} />
  {{/each}}
</main>

<Footer>
  {{#link-to "new"}}
    <Icons as |Icon|>
      <Icon.PlusSquare />
    </Icons>
  {{/link-to}}
</Footer>
